{
  "expo": {
    "name": "JOBSY",
    "slug": "jobsy-app",
    "version": "1.0.0",
    "scheme": "jobsy",
    "orientation": "portrait",
    "icon": "./assets/images/logo/logo-jobsy-favicon-jobsy-nyb.png",
    "userInterfaceStyle": "dark",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/logo/logo-jobsy-favicon-jobsy-nyb.png",
      "resizeMode": "contain",
      "backgroundColor": "#BEB52C"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "softwareKeyboardLayoutMode": "pan",
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.anonymous.proyectoappmobilefcaduner2025",
      "permissions": [
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.ACCESS_FINE_LOCATION"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-secure-store",
      "expo-font",
      [
        "expo-image-picker",
        {
          "photosPermission": "Jobsy necesita acceso a tu galería para elegir una foto de perfil.",
          "cameraPermission": "Jobsy necesita acceso a tu cámara para tomar una foto de perfil."
        }
      ],
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Permitir que Jobsy acceda a tu ubicación",
          "isIosBackgroundLocationEnabled": "Permitir que Jobsy acceda a la geolocalización en background."
        }
      ],
      [
        "expo-camera",
        {
          "cameraPermission": "Jobsy necesita acceso a tu cámara para tomar fotos de perfil y fotos para ofertas laborales."
        }
      ]
    ],

    "extra": {
      "eas": {
        "projectId": "e001ab00-af23-4a4b-9647-a39f8f0ce724"
      }
    },
    "owner": "dreamteamdevs"
  }
}
